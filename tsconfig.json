{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "target": "es6",
    "outDir": "out",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "rootDir": "."
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
    "examples"
  ]
}