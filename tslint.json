{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "quotemark": [
      "single"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always",
        "method": "never",
        "constructor": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "ordered-imports": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ]
  }
}
