{
  "parserOptions": {
    "ecmaVersion": 5
  },
  "extends": [
    "airbnb-base/legacy"
  ],
  "rules": {
    "max-len": [
      2,
      {
        "code": 100
      }
    ],
    "no-console": 0,
    "no-restricted-syntax": [
      0,
      "ForInStatement"
    ],
    "func-names": 0,
    "vars-on-top": 0
  }
}